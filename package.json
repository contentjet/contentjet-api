    "type": "git",
    "url": "https://github.com/contentjet/contentjet-api"
  },
  "private": false,
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run build && concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
